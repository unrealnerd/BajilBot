{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Twilio": {
    "TwilioAuthToken": "",
    "TwilioAccountSid": "",
    "TwilioFromPhoneNumber": ""
  },
  "Telegram": {
    "TelegramBaseAddress": "https://api.telegram.org/bot",
    "TelegramAPIKey": ""
  },
  "Slack": {
    "SlackBaseAddress": "https://slack.com/api",
    "SlackAPIKey": ""
  },
  "Features": {
    "NLUEnabled": "false"
  },
  "FeatureProvider": {
    "RedisServer": "",
    "DefaultFeatureSource": "config"
  },
  "ServicesSettings": {
    "services": [{
        "name": "CorporateBuzzWord",
        "isParent": true,
        "url": "https://corporatebs-generator.sameerkumar.website",
        "tags": ["corporate", "buzzword", "bullshit", "api"],
        "services": [{
          "url": "/",
          "description": "get a random corporate buzzword",
          "response": {
            "mappings": {
              "quote": "phrase"
            }
          }
        }]
      },
      {
        "name": "OpenDota",
        "isParent": true,
        "url": "https://api.opendota.com/api",
        "tags": ["dota", "opendota", "api"],
        "description": "Dota related API",
        "services": [{
            "url": "/proMatches",
            "description": "get 100 recent promatches",
            "tags": ["promatches"],
            "response": {
              "isArray": true
            },
            "responseTemplate": "DATAGRID"
          },
          {
            "url": "/matches/{{id}}",
            "description": "get a match by ID",
            "tags": ["matches", "match"],
            "responseTemplate": "DATAGRID"
          }
        ]
      },
      {
        "Name": "RandomDog",
        "isParent": true,
        "url": "https://dog.ceo/api",
        "services": [{
          "url": "/breeds/image/random",
          "description": "get a random dog image",
          "tags": ["dog", "random", "breed", "randombreed"],
          "responseTemplate": "IMAGE",
          "response": {
            "mappings": {
              "imageUrl": "message"
            }
          }
        }]
      },
      {
        "name": "DBservice",
        "isParent": true,
        "url": "http://localhost:6000",
        "tags": ["db"],
        "description": "DB related API",
        "services": [{
          "url": "/",
          "method": "POST",
          "description": "saves to DB",
          "tags": ["save"],
          "request": {
            "template": "{\"header\":\"{{jsonobject}}\",\"body\":{\"key\":\"{{string}}\",\"value\":\"{{jsonobject}}\"}}"
          },
          "response": {
            "isArray": false
          },
          "responseTemplate": "QUOTE"
        }],
        "hide": true
      },
      {
        "name": "InventoryService",
        "isParent": true,
        "url": "http://localhost:6000/api",
        "tags": ["product"],
        "description": "Inventory repository service",
        "services": [{
          "url": "/product",
          "method": "POST",
          "description": "adds a new product",
          "tags": ["save"],
          "request": {
            "template": {
              "name": "{{string}}",
              "code": "{{string}}",
              "description": "{{string}}",
              "unitPrice": "{{float}}",
              "images": "[{{string}}]",
              "tags": "[{{string}}]"
            }
          },
          "response": {
            "isArray": false
          },
          "responseTemplate": "QUOTE"
        }],
        "hide": false
      }
    ]
  }
}